    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.6.0",
    "jsdom": "^24.1.0",
    "lefthook": "^1.6.16",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1",
    "vercel": "^34.2.7",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "wrangler": "^3.60.3"
