{
  "name": "pressure-cooker-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "packageManager": "pnpm@9.1.2",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "pnpm run build && wrangler pages deploy ./build/client",
    "dev": "remix vite:dev",
    "start": "wrangler pages dev ./build/client",
    "check": "concurrently 'pnpm run lint:*' 'tsc'",
    "lint": "concurrently 'pnpm run lint:*'",
    "lint:eslint": "eslint . --max-warnings 0 --config eslint.config.mjs",
    "lint:biome": "biome check .",
    "fmt": "biome fmt .",
    "typecheck": "tsc",
    "cf-typegen": "wrangler types",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.9.2",
    "@remix-run/cloudflare-pages": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "isbot": "^5.1.6",
    "miniflare": "^3.20240512.0",
    "next": "^14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@cloudflare/next-on-pages": "^1.11.3",
    "@cloudflare/workers-types": "^4.20240512.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.3.0",
    "@remix-run/dev": "^2.9.2",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^15.3.0",
    "jsdom": "^24.0.0",
    "lefthook": "^1.6.12",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0",
    "vercel": "^34.2.0",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "wrangler": "^3.57.0"
  },
  "engines": {
    "node": ">=20.13.1",
    "pnpm": ">=9.1.2",
    "npm": "please use pnpm!",
    "yarn": "please use pnpm!"
  }
}
